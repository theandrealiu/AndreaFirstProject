import java.util.ArrayList;


public class Digits {
	
	private ArrayList<Integer> digitList = new ArrayList(0);
	private int number;
	private String output = "";
	
	public Digits(){
	}
	
	public int Digits(int num){
		number = num;
		if (number != 0){
			while (number >0){
				int dig = number%10;
				digitList.add(0,dig);
				number = number/10;
			}
		}
		return number;
		//else{
			//digitList.add(0,0);
		//}
		//for (int i = 0; i <digitList.size(); i++){
			//output = output + digitList.get(i) + " ";
		//}
	}
}


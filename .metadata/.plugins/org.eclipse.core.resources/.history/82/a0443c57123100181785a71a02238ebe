import java.io.File;
import java.io.IOException;
import java.util.Scanner;
import static java.lang.System.*;

public class Maze
{
   private int[][] maze;

	public Maze()
	{
	}

	public Maze(int size, String line)
	{
		maze = new int[size][size];
		String[] numbers = new String[size * size];
		numbers = line.split(" ");
		for (int i = 0; i < size; i++){
			for (int j = 0; j < size; j++){
			}
		}
	}

	public boolean hasExitPath(int r, int c)
	{
		if (maze[r][c] == '1'){
			maze[r][c] = '0';
		}
		if (r == maze.length - 1){
			return true;
		}
		else{
			int newR = 0;
			int newC = 0;
			if(r!=9){
				newR = r+1;
				hasExitPath(newR,c);
			}
			if(r!=0){
				newR = r-1;
				hasExitPath(newR,c);
			}
			if(c!=9){
				newC = c+1;
				hasExitPath(r,newC);
			}
			if(c!=0){
				newC = c-1;
				hasExitPath(r,newC);
			}
		}
		return false;
	}

	public String toString()
	{
		String output="";
		for (int i = 0; i < maze.length; i++){
			for (int j = 0; j < maze.length; j++){
				out.print(maze[i][j]);
			}
			out.println();
		}
		return output;
	}
}